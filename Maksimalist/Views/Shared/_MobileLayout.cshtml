        $(document).ready(function () {
            //no facebook event for sharing a link.
            $('a.fb').on('click', function () {
                _gaq.push(['_trackSocial',
                    'facebook',
                    'Share Button',
                    parseFBHref($(this).attr('href'))
                ]);
            });

            function replaceAll(find, replace, str) {
                return str.replace(new RegExp(find, 'g'), replace);
            }

            function parsePinterestHref(href) {
                href = replaceAll('%3A', ':', href);
                href = replaceAll('%2F', '/', href);
                if (href.indexOf('url=') !== -1 && href.indexOf('&media=') !== -1) {
                    href = href.substring((href.indexOf('url=') + 4), href.indexOf('&media='));
                }
                return href;
            }

            function parseTwitterHref(href) {
                href = replaceAll('%3A', ':', href);
                href = replaceAll('%2F', '/', href);
                if (href.indexOf('url=') !== -1 && href.indexOf('&text') !== -1) {
                    href = href.substring((href.indexOf('url=') + 4), href.indexOf('&text'));
                }
                return href;
            }

            function parseFBHref(href) {
                href = replaceAll('%3A', ':', href);
                href = replaceAll('%2F', '/', href);
                if (href.indexOf('u=') !== -1) {
                    href = href.substring((href.indexOf('u=') + 2));
                }
                return href;
            }

            //no pinterest api, track clicks on pinterest button
            $('.pin').on('click', function () {
                _gaq.push(['_trackSocial',
                    'pinterest',
                    'Pin button',
                    parsePinterestHref($(this).attr('href'))
                ]);
            });

            //twitter
            twttr.ready(function (twttr) {
                // Now bind our custom intent events
                twttr.events.bind('click', clickIntentToAnalytics);
                twttr.events.bind('tweet', tweetIntentToAnalytics);
                twttr.events.bind('retweet', retweetIntentToAnalytics);
                twttr.events.bind('favorite', favoriteIntentToAnalytics);
                twttr.events.bind('follow', followIntentToAnalytics);
            });

            function tweetIntentToAnalytics() {
                var href = $("meta[name='twitter:url']").attr('content');
                _gaq.push(['_trackSocial', 'twitter', 'tweet', href]);
            }

            function clickIntentToAnalytics() {
                var href = $("meta[name='twitter:url']").attr('content');
                _gaq.push(['_trackSocial', 'twitter', 'click', href]);
            }

            function retweetIntentToAnalytics() {
                var href = $("meta[name='twitter:url']").attr('content');
                _gaq.push(['_trackSocial', 'twitter', 'retweet', href]);
            }

            function favoriteIntentToAnalytics() {
                var href = $("meta[name='twitter:url']").attr('content');
                _gaq.push(['_trackSocial', 'twitter', 'favorite', href]);
            }

